{
  "name": "STM32F4_Discovery_Audio_Codec",
  "version": "1.1.0",
  "description": "Low-level driver for the CS43L22 Audio Codec used in STM32F4 Discovery Kit.",
  "keywords": [
    "stm32f4",
    "audio",
    "codec",
    "cs43l22",
    "stm32f4-discovery"
  ],
  "authors": [
    {
      "name": "MCD Application Team",
      "email": "support@st.com",
      "url": "https://www.st.com",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "frameworks": "spl",
  "platforms": "ststm32",
  "dependencies": {
    "CMSIS": "*"
  },
  "build": {
    "flags": [
      "-I lib/audio_codec",
      "-D USE_STM32F4_DISCOVERY_CODEC"
    ],
    "srcFilter": [
      "+<*>"
    ]
  }
}
